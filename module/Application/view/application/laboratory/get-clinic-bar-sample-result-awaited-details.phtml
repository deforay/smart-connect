<script>
       $(function () {
              $('#clinicbarcontainer').highcharts({
              chart: {
                   type: 'column'
              },
              title: {
                   text: '<?php echo $this->translate('Results Awaited (Health Center-wise)'); ?>'
              },
              exporting:{
                     sourceWidth: 1200,
                     sourceHeight: 400,
                     chartOptions:{
                         subtitle:null
                     }
              },
              credits: {
                     enabled: false
              },
               xAxis: {
                   categories: [<?php
                   if(isset($result['clinic']) && count($result['clinic'])>0){
                     foreach($result['clinic'] as $clinic){
                         echo "'".$clinic."',";
                     }
                   }
                   ?>]
               },
               yAxis: {
                   allowDecimals: false,
                   min: 0,
                   title: {
                       text: '<?php echo $this->translate('No. of Samples');?>'
                   },
                   labels: { formatter: function() { return this.value; } },
               },
               legend: {
                     enabled: true
               },
               tooltip: {
                   formatter: function () {
                       return '<b>' + this.x + '</b><br/>' +
                           this.series.name + ': ' + this.y
                   }
               },
               plotOptions: {
                   column: {
                       stacking: 'normal',
                       pointPadding: 0.2,
                       borderWidth: 0
                   }
               },
               series: [<?php
                  if(isset($result['sample']) && count($result['sample']) >0){
                     foreach($result['sample'] as $key=>$condition){
                     ?>
                     {
                     showInLegend: false,
                     name:'<?php echo $this->translate('Samples');?>',
                     data:[<?php
                         foreach($condition as $count){
                            echo '{y:'.$count.'},';
                         }
                     ?>],
                     },
                     <?php
                     }
                  }
                   ?>],
                   colors: ['#f7a35c']
           });
      });
</script>